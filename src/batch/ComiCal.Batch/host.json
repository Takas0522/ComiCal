{
    "version": "2.0",
    "extensions": {
      "durableTask": {
        "storageProvider": {
          "type": "azureStorage",
          "connectionStringName": "AzureWebJobsStorage"
        }
      }
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        }
      },
      "logLevel": {
        "Default": "Information",
        "Microsoft.Azure.Functions.Worker": "Debug",
        "ComiCal.Batch.Repositories.RakutenComicRepository": "Error"
      }
    }
}